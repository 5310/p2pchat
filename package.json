{
  "name": "p2pchat",
  "version": "0.0.0",
  "main": "dist/index.html",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.5.1",
    "parcel-plugin-pug": "^0.2.4"
  },
  "dependencies": {
    "libp2p": "^0.15.2",
    "libp2p-floodsub": "^0.13.1",
    "libp2p-multiplex": "^0.5.1",
    "libp2p-railing": "^0.7.1",
    "libp2p-secio": "^0.9.1",
    "libp2p-webrtc-star": "^0.13.3",
    "libp2p-websockets": "^0.10.4",
    "material-design-lite": "^1.3.0",
    "peer-info": "^0.11.6",
    "pify": "^3.0.0"
  },
  "scripts": {
    "dev": "parcel src/index.pug --https",
    "cachelist": "ls -1 dist | sed 's/^/  \"/g' | sed 's/$/\"/g' | sed '1s/^/[\\n/;$!s/$/,/;$s/$/\\n]/' > dist/cachelist.json",
    "build": "rm -rf dist/* && parcel build --no-minify --public-url / src/index.pug && yarn cachelist",
    "deploy": "surge -p dist -d https://p2pchat.surge.sh"
  }
}
